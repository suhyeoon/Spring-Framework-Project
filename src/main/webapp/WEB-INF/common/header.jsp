	function fnSearch() {
		var Branch = "";
		$(".selectSt06").each(function () {
			if ($(this).val() != "") {
				Branch = $(this).val();
			}
		});
		location.href = "/Offers/List.aspx?branch=" + Branch
                        + "&pkgType=" + $("#hdnPkgType").val()
                        + "&SDate=" + $("#txtSDatePop").val()
                        + "&EDate=" + $("#txtEDatePop").val()
                        + "&AdultCnt=" + $("#selAdultCntPop").val()
                        + "&ChildCnt=" + $("#selChildCntPop").val()
                        + "&RoomCnt=" + $("#selRoomCntPop").val()
	}

	function ReserOn() {
		var UrlCheck = 'M';

		if (UrlCheck != "N" && UrlCheck != "P") {
			if ($("#reserBox").css("display") == "none") {
				$('#reserBox').show();
			}
			else {
				$('#reserBox').hide();
			}
		} else if (UrlCheck == "P") {
			location.href = "/Offers/List.aspx"
		}
	}

